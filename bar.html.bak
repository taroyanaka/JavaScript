<!DOCTYPE html>
<html lang="ja">
<head>
  <meta http-equiv="content-type" content="text/html; charset=utf-8">
  <title>bar</title>
  <script>
    // FUNCTIONS
    function* pages(from, to) {while (from <= to) yield from++};
    const DQSA = document.querySelector.bind(document);
    const DCE = document.createElement.bind(document);
    const GEBI = document.getElementById.bind(document);
    // DATA
    const URL = 'https://firestore.googleapis.com/v1beta1/projects/taro-fe94c/databases/(default)/documents/porncom/';
  </script>
</head>
<body onLoad='init()'>
  <div id="foo"></div>
  <script>
    init = () => {
      //paging and access elements
      for (var num of pages(0, 1)) {        (() => {          fetch(URL + num.toString()).then(res => {            return res.json()          }).then((txt) => {            var div = DCE('div'); div.innerHTML = txt["fields"]["views"]["integerValue"]; GEBI('foo').appendChild(div);          })        })();      }
    }
  </script>
</body>
</html>