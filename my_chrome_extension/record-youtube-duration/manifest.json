{
  "name": "record-youtube-duration",
  "action": {},
  "manifest_version": 3,
  "version": "0.1",
  "description": "chrome extension for youtube cut-out timing recorder, Ctrl+Shift+P or click the button, result is chrome console",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "windows": "Ctrl+Shift+P",
        "mac": "Command+Shift+P",
        "chromeos": "Ctrl+Shift+P",
        "linux": "Ctrl+Shift+P"
      }
    }
  }
}